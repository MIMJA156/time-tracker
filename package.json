{
  "icon": "image.png",
  "name": "time-tracker-app",
  "displayName": "Mimjas Time Tracker",
  "description": "Logs the amount of time you spend coding.",
  "version": "0.3.4",
  "publisher": "MIMJA156",
  "repository": {
    "type": "git",
    "url": "https://github.com/MIMJA156/time-tracker.git"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "configuration": [
      {
        "title": "Mimjas Time Tracker",
        "properties": {
          "Icon Style": {
            "type": "string",
            "default": "circuit-board",
            "description": "Type in the default codicon ID for the icon you want.\nList of icons -> https://code.visualstudio.com/api/references/icons-in-labels#icon-listing",
            "scope": "application"
          },
          "Label Position": {
            "type": "string",
            "enum": [
              "Left",
              "Right"
            ],
            "default": "Left",
            "description": "Defines the position on the time label."
          },
          "Label Priority": {
            "type": "boolean",
            "default": "false",
            "description": "Is high priority"
          }
        }
      }
    ]
  },
  "devDependencies": {
    "eslint": "^8.6.0"
  },
  "dependencies": {
    "chart.js": "^3.7.0",
    "express": "^4.17.2"
  }
}
